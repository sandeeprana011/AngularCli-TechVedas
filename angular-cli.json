{
  "project": {
    "version": "1.0.0-beta.17",
    "name": "angular-cli-surveyam"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "diy",
      "assets": [
        "assets",
        "favicon.ico",
        "app/images",
        "app/css",
        "../static"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.css",
        "awesomplete.css"
      ],
      "scripts": [
        "awesomplete.js",
        "materialize.js",
        "../node_modules/chart.js/dist/Chart.bundle.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false
  }
}
